// These are Workplace settings which are specific to the NEARoracle project
// and are shared among developers on the project. The settings in this file
// override default and user settings

{
    "editor.wordWrap": "on",
    "files.trimTrailingWhitespace": true,
    "python.formatting.provider": "autopep8",
    "python.formatting.autopep8Args": ["--max-line-length", "120"],
    "editor.formatOnSave": true,
    "python.linting.enabled": true,
}